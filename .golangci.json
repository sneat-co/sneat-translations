{
  "formatters": {
    "exclusions": {
      "generated": "lax",
      "paths": [
        "third_party$",
        "builtin$",
        "examples$"
      ]
    }
  },
  "linters": {
    "exclusions": {
      "generated": "lax",
      "paths": [
        "third_party$",
        "builtin$",
        "examples$"
      ],
      "presets": [
        "comments",
        "common-false-positives",
        "legacy",
        "std-error-handling"
      ]
    },
    "settings": {
      "staticcheck": {
        "checks": [
          "all",
          "-ST1003",
          "-ST1018"
        ]
      }
    }
  },
  "version": "2"
}
